a=int(input("enter the choice"))
if a in range(1,5):
 b=int(input("enter number"))
 c=int(input("enter number"))
match a:
    case 1:
        print("add:",b+c)
    case 2:
        print("subtract:", b - c)
    case 3:
        print("multiply:", b * c)
    case 4:
        print("divide:", b / c)
    case 5:
        print("exit")
    case _:
        print("wrong choice u entered")
